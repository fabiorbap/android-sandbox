<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.frachid.sandbox">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".SandboxApplication"
        android:theme="@style/AppTheme">
        <activity android:name="br.com.frachid.sandbox.view.viewcontrollers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="br.com.frachid.sandbox.view.viewcontrollers.BaseActivity" />

    </application>

</manifest>